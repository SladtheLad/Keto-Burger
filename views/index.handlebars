<div class="row">
  <div class="col s6 m3 l3 offset-m2 offset-l2">
    <ul class="collection">
      <li class="collection-header">
        <h4>Burgers to Devour!</h4>
      </li>
      {{#each notEaten}}
      <li class="collection-item">
        <p>
          {{this.id}}. {{this.burger_name}}
          <button data-id={{this.id}} class="del-btn btn btn-warning">Devour</button>
        </p>
      </li>
      {{/each}}
    </ul>
  </div>


  <div class="col s6 m4 l4">
    <ul class="collection">
      <li class="collection-header">
        <h4>Burgers that have been Devoured!</h4>
      </li>
      {{#each eaten}}
      <li class="collection-item">
        <p>
          {{this.id}}. {{this.burger_name}} has been devoured!
        </p>
      </li>
      {{/each}}
    </ul>
  </div>
</div>